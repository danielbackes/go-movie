{
  "expo": {
    "name": "Go Movie ALMT",
    "description": "Aplicativo desenvolvido durante o treinamento",
    "slug": "go-movie-almt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.danielbackes.gomoviealmt"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.danielbackes.gomoviealmt"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
